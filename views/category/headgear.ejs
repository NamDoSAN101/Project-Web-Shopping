<%- include("../partials/header.ejs") %>

<div class="album py-5 bg-light">
    <div class="container">

      <div class="row row-cols-1 row-cols-sm-2 row-cols-md-3 g-3">
      <% index.forEach(function(headgear){ %>
        <div class="col">
          <div class="card shadow-sm">
            <img src="<%= index.image %>" height="300px" class="index-image">
            <div class="card-body">
              <p class="card-text">Item: <%= index.name %></p>
              <p class="card-text">ref: <%= index.Artist %></p>
              <div class="d-flex justify-content-between align-items-center">
                <div class="btn-group">
                  <a href="/prints/<%= prints._id %>" class="btn btn-sm btn-outline-secondary">View</a>
                </div>
                <small class="text-muted"><%= prints.price %> $</small>
              </div>
            </div>
          </div>
        </div>
        <% }) %>
      </div>
    </div>
  </div>

<%- include("../partials/footer.ejs") %>